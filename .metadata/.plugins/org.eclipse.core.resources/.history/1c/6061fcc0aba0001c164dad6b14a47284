function eliminar(id){
	swal({
  title: "Est치 seguro de eliminar el registro seleccionado?",
  text: "Los cambios no se podr치n rehacer",
  icon: "warning",
  buttons: true,
  dangerMode: true,
})
.then((OK) => {
  if (OK) {
	$.ajax({
		url:"/eliminar/"+id,
		success: function(res){
			console.log(res);
		}
	});
    swal("Se elimin칩 el registro correctamente", {
      icon: "success",
    }).then((ok)=>{
		if(ok){
			location.href="/listar";
		}
	});
	  } else {
	    swal("No se elimin칩 el archivo");
	  }
	});
}